@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>Fiby-Portal Web</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="~/cssadmin/assets/images/favicon.png">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-alpha1/dist/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

    <link rel="stylesheet" type="text/css" href="~/css/login/css/main.css">
    <!--===============================================================================================-->
</head>
<body>

    <div class="loginBox">
        <img class="user" src="~/img/logo-modified.png" height="100px" width="100px">
        <h3>Portal Web</h3>
        @*<form action="login.php" method="post">*@
            <div class="inputBox"> 
                <input type="text" name="txtUserName" id="txtUserName" placeholder="Usuario"> 
                <input type="password" name="Password" id="txtPassword" placeholder="Contraseña"> 
            </div> 
            <input type="submit" name="" id="btnIngresarSistema" value="Ingresar">
        @*</form>*@ 
        @*<a href="#">Forget Password<br> </a>
        <div class="text-center">
            <p style="color: #59238F;">Sign-Up</p>
        </div>*@
    </div>
    <input type="hidden" value="@ViewBag.Error" id="hdLoginError" />
</body>
</html>
<script type="text/javascript" src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-alpha1/dist/js/bootstrap.bundle.min.js"></script>
<script type="text/javascript" src="@Url.Content("~/js/jquery.min.js")"></script>
<script type="text/javascript" src="@Url.Content("~/app/views/account/login.js")" asp-append-version="true"></script>
